<script>

//faz a validação do usuário para verificar se ele está logado, se não estiver, volta para tela de login barrando a entrada na home
    function valideLogin(){

        const token = window.sessionStorage.getItem('token');

        if (token === undefined | token === null) {
            window.location.href = "/login";
        }
    }

    valideLogin()

</script>